section.section.Partner
  .container 
    .Partner-wrapper 
      .section-header
        h3.section-title.wow.animate__bounceIn Partner 

      .Partner-main.flex.flex-wrap.justify-between
        each item in [1,2,3,4,5,6,7,8]
          .Partner-main-item.wow.animate__fadeInUp(data-wow-delay=`0.${item}s`)
            img(src=`./assets/images/image-partner-${item}.png`, alt="")